{
    "source": "./src",
    "destination": "./doc",
    "excludes": ["^sample/"],
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "test": {
                    "source": "./test",
                    "type": "mocha"
                }
            }
        },
        {
            "name": "esdoc-ecmascript-proposal-plugin",
            "option": {"all": true}
        },
        {
            "name": "esdoc-flow-type-plugin"
        }
    ]
}
