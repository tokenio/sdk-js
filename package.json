{
  "name": "token-io",
  "version": "1.4.127",
  "description": "",
  "main": "dist/token-io.min.js",
  "scripts": {
    "postinstall-script": "./config/patch.sh",
    "postinstall": "npm run postinstall-script",
    "lint": "./node_modules/eslint/bin/eslint.js -c ./.eslintrc.js ./src ./test",
    "test": "npm run testNode",
    "testBrowser": "npm run lint && ./node_modules/karma/bin/karma start test/karma.config.js",
    "testBrowserDev": "TEST_ENV=dev ./node_modules/karma/bin/karma start test/karma.config.js",
    "testBrowserStg": "TEST_ENV=stg ./node_modules/karma/bin/karma start test/karma.config.js",
    "testBrowserSandbox": "TEST_ENV=sandbox ./node_modules/karma/bin/karma start test/karma.config.js",
    "testBrowserPrd": "TEST_ENV=prd ./node_modules/karma/bin/karma start test/karma.config.js",
    "testNode": "npm run lint && node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors \"test/**/*.spec.js\"",
    "testNodeDev": "npm run lint && TEST_ENV=dev node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors \"test/**/*.spec.js\"",
    "testNodeStg": "npm run lint && TEST_ENV=stg node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors \"test/**/*.spec.js\"",
    "testNodeSandbox": "npm run lint && TEST_ENV=sandbox node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors \"test/**/*.spec.js\"",
    "testNodePrd": "npm run lint && TEST_ENV=prd node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors \"test/**/*.spec.js\"",
    "bamboo": "npm run lint && node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "bambooDev": "npm run lint && TEST_ENV=dev node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "bambooStg": "npm run lint && TEST_ENV=stg node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "bambooSandbox": "npm run lint && TEST_ENV=sandbox node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "bambooPrd": "npm run lint && TEST_ENV=prd node_modules/mocha-webpack/bin/mocha-webpack --webpack-config config/webpack.config.node.js --timeout 60000 --colors -R mocha-bamboo-reporter  \"test/**/*.spec.js\"",
    "build": "npm run buildBrowser && npm run buildNode && WEBPACK_ENV=build npm run buildBrowser",
    "buildBrowser": "npm run lint && TEST_ENV=prd webpack --config config/webpack.config.js",
    "buildBrowserw": "npm run lint && TEST_ENV=prd webpack --config config/webpack.config.js --progress --colors -w",
    "buildNode": "npm run lint && TEST_ENV=prd webpack --config config/webpack.config.node.js",
    "buildNodew": "npm run lint && TEST_ENV=prd webpack --config config/webpack.config.node.js --progress --colors -w",
    "doc": "./node_modules/esdoc/out/src/ESDocCLI.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "esdoc": "^1.0.3",
    "esdoc-inject-script-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^3.6.1",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^4.0.0",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.0",
    "karma-webpack": "^1.8.0",
    "lodash": "^4.16.2",
    "mocha": "^3.0.2",
    "mocha-bamboo-reporter": "^1.1.1",
    "mocha-webpack": "^0.6.0",
    "npm-cli-login": "0.0.10",
    "sinon": "^1.17.6",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-regenerator-runtime": "^6.5.0",
    "base64-js": "^1.2.0",
    "base64url": "^2.0.0",
    "bs58": "^4.0.1",
    "buffer": "3.4.3",
    "es6-promise": "^3.3.1",
    "fast-sha256": "^1.0.0",
    "json-loader": "^0.5.4",
    "json-stable-stringify": "^1.0.1",
    "sjcl": "^1.0.6",
    "tweetnacl": "^0.14.3"
  }
}
